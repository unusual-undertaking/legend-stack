{
    "name": "convex-tanstack-starter-kit",
    "private": true,
    "type": "module",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "turbo dev",
        "build": "turbo build",
        "dev:web": "turbo -F web dev",
        "dev:backend": "turbo -F @convex-tanstack-starter-kit/backend dev",
        "dev:setup": "turbo -F @convex-tanstack-starter-kit/backend dev:setup"
    },
    "devDependencies": {
        "turbo": "^2.6.3"
    },
    "packageManager": "npm@11.3.0"
}
